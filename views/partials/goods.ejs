    <div class="section scrollspy" id="work">
        <div class="container">
            <h2 class="header text_b">Food </h2>
            <div class="row">
                
                <% studentStore.food.forEach(function(item){ %>
                
                <div class="col s12 m4 l4">
                    <div class="card">
                        <div class="card-image waves-effect waves-block waves-light">
                            <img class="activator" style="height:300px;" src="img/food<%= item.id%>.jpg">
                        </div>
                        <div class="card-content">
                            <span class="card-title activator grey-text text-darken-4"> <%= item.name %> <i class="mdi-navigation-more-vert right"></i></span>
                            <p><a href="#"> $ <%= item.price %> </a></p>
                        </div>
                        <div class="card-reveal">
                            <span class="card-title grey-text text-darken-4"> <%= item.name %> <i class="mdi-navigation-close right"></i></span>
                            <p></p>
                            <p> <%= item.description %> </p>
                        </div> 
                    </div>
                </div>
                
                <% }) %>
            </div>
        </div>  
    </div>  

    <div class="section scrollspy" id="clothes">
        <div class="container">
            <h2 class="header text_b">Clothes </h2>
            <div class="row">
                
                <% studentStore.clothes.forEach(function(item){ %>
                
                <div class="col s12 m4 l4">
                    <div class="card">
                        <div class="card-image waves-effect waves-block waves-light">
                            <img class="activator" style="height:300px;" src="img/clothes<%= item.id%>.jpg">
                        </div>
                        <div class="card-content">
                            <span class="card-title activator grey-text text-darken-4"> <%= item.name %> <i class="mdi-navigation-more-vert right"></i></span>
                            <p><a href="#"> $ <%= item.price %> </a></p>
                        </div>
                        <div class="card-reveal">
                            <span class="card-title grey-text text-darken-4"> <%= item.name %> <i class="mdi-navigation-close right"></i></span>
                            <p></p>
                            <p> <%= item.description %> </p>
                        </div> 
                    </div>
                </div>
                
                <% }) %>
            </div>
        </div> 
    </div> 

     
    <div class="section scrollspy" id="supplies">
        <div class="container">
            <h2 class="header text_b">Simon Tech Supplies </h2>
            <div class="row">
                
                <% studentStore.simontech.forEach(function(item){ %>
                
                <div class="col s12 m4 l4">
                    <div class="card">
